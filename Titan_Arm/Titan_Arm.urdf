	<!-- * * * Joint Definitions * * * -->
  
	
	<joint name="fixedbase" type="fixed">
    	<parent link="base_link"/>
    	<child link="upper_base_link"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>
	
	<joint name="fixed1" type="fixed">
    	<parent link="upper_base_link"/>
    	<child link="link1"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>
	
	<joint name="revjoint1" type="revolute">
    	<parent link="link1"/>
    	<child link="rev1"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-2.356" upper="2.356" effort="1" velocity="0.01"/>
  	</joint>
  	
	<joint name="fixed2" type="fixed">
    	<parent link="rev1"/>
    	<child link="link2"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>
	
	<joint name="fixed3" type="fixed">
    	<parent link="link2"/>
    	<child link="rev2"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>	
	
	<joint name="revjoint2" type="revolute">
    	<parent link="rev2"/>
    	<child link="link3"/>
    	<origin xyz="0 0.48 0" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-2.356" upper="2.356" effort="1" velocity="0.01"/>
  	</joint>
  	
	<joint name="fixed4" type="fixed">
    	<parent link="link3"/>
    	<child link="pris1"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>
		
	<joint name="prisjoint1" type="prismatic">
    	<parent link="pris1"/>
    	<child link="link4"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
       	<limit lower="-0.26" upper="0.2" effort="1" velocity="0.01"/>
        <axis xyz="0 0 1"/>
	</joint>
	
	<joint name="revjoint3" type="revolute">
    	<parent link="link4"/>
    	<child link="endef1"/>
    	<origin xyz="0 0.48 0" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-6.28319" upper="6.28319" effort="1" velocity="0.01"/>
  	</joint>
  	
	<joint name="fixed5" type="fixed">
    	<parent link="endef1"/>
    	<child link="endef2"/>
    	<origin xyz="0 -0.12 0" rpy="0 0 0"/>
	</joint>
		
	<joint name="fixed6" type="fixed">
    	<parent link="endef2"/>
    	<child link="grip1"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>	
	
