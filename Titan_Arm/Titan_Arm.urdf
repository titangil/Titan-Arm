	<joint name="prisjoint2" type="prismatic">
    	<parent link="grip1"/>
    	<child link="gripL"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
       	<limit lower="-0.04" upper="0" effort="10" velocity="0.3"/>
        <axis xyz="1 0 0"/>
	</joint>
	
	<joint name="prisjoint3" type="prismatic">
    	<parent link="grip1"/>
